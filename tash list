[32m+[m	[32m<!-- 子组件模板 -->[m
[32m+[m[32m<template id="child-template">[m
[32m+[m[32m  <input v-model="msg1">[m
[32m+[m[32m  <button v-on:click="notify">Dispatch Event</button>[m
[32m+[m[32m</template>[m
[32m+[m
[32m+[m[32m<!-- 父组件模板 -->[m
[32m+[m[32m<div id="events-example">[m
[32m+[m[32m  <p>Messages: {{ messages | json }}</p>[m
[32m+[m[32m  <child></child>[m
[32m+[m[32m</div>[m
[32m+[m
[32m+[m	[32m<script>[m
[32m+[m[32m// 注册子组件[m
[32m+[m[32m// 将当前消息派发出去[m
[32m+[m[32mVue.component('child', {[m
[32m+[m[32m  template: '#child-template',[m
[32m+[m[32m  data: function () {[m
[32m+[m[32m    return { msg1: 'hello' }[m
[32m+[m[32m  },[m
[32m+[m[32m  methods: {[m
[32m+[m[32m    notify: function () {[m
[32m+[m[32m      if (this.msg1.trim()) {[m
[32m+[m[32m        this.$dispatch('child-msg', this.msg1)[m
[32m+[m[32m        this.msg1 = ''[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32m// 初始化父组件[m
[32m+[m[32m// 将收到消息时将事件推入一个数组[m
[32m+[m[32mvar parent = new Vue({[m
[32m+[m[32m  el: '#events-example',[m
[32m+[m[32m  data: {[m
[32m+[m[32m    messages: [][m
[32m+[m[32m  },[m
[32m+[m[32m  // 在创建实例时 `events` 选项简单地调用 `$on`[m
[32m+[m[32m  events: {[m
[32m+[m[32m    'child-msg': function (msg1) {[m
[32m+[m[32m      // 事件回调内的 `this` 自动绑定到注册它的实例上[m
[32m+[m[32m      this.messages.push(msg1)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m})[m
[32m+[m	[32m</script>[m
